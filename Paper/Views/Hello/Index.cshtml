@section scripts{

    <script>

        function showtab(href)
        {
            //change tab
            //alert('yes, the click actually happened');
            $('.nav-tabs a[href="#' + href + '"]').tab('show');

            //scoll to top
            $('body,html').animate({
                scrollTop: 150,
            }, 0
                );
        }

        function numbericTextBox(selector, increasement, fixedpoint) {
            $(selector + ' .btn:first-of-type').on('click', function () {
                var input = $(selector + ' input');
                input.val((Number($(selector + ' input').val()) + increasement).toFixed(fixedpoint));

                //trigger for angular
                input.trigger('input');
            });
            $(selector + ' .btn:last-of-type').on('click', function () {
                var input = $(selector + ' input');
                input.val((Number($(selector + ' input').val()) - increasement).toFixed(fixedpoint));

                //trigger for angular
                input.trigger('input');

            });
        }

        function percentTextBox(selector, increasement, fixedpoint) {

            $(selector + ' .btn:first-of-type').on('click', function () {
                var input = $(selector + ' input');
                input.val((Number($(selector + ' input').val().replace('%', '')) + increasement).toFixed(fixedpoint) + "%");

                //trigger for angular
                input.trigger('input');
            });
            $(selector + ' .btn:last-of-type').on('click', function () {
                var input = $(selector + ' input');
                input.val((Number($(selector + ' input').val().replace('%', '')) - increasement).toFixed(fixedpoint) + "%");

                //trigger for angular
                input.trigger('input');
            });
        }

        $(document).ready(function () {

            /*var select = '';
            for (i = 1; i <= 100; i++) {
                select += '<option val=' + i + '>' + i + '</option>';
            }
            $('#some_selector').html(select);*/

