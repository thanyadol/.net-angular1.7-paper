        function showtab(href) {
            //change tab
            //alert('yes, the click actually happened');
            $('.nav-tabs a[href="#' + href + '"]').tab('show');

            //scoll to top
            $('body,html').animate({
                scrollTop: 150,
            }, 0
                );
        }

        function numbericTextBox(selector, increasement, fixedpoint) {
            $(selector + ' .btn:first-of-type').on('click', function () {
                var input = $(selector + ' input');

                var values = (Number($(selector + ' input').val()) + increasement).toFixed(fixedpoint);
                if (values < 0)
                    input.val(0);
                else
                    input.val(values);

                //trigger for angular
                input.trigger('input');
            });

            $(selector + ' .btn:last-of-type').on('click', function () {
                var input = $(selector + ' input');

                var values = (Number($(selector + ' input').val()) - increasement).toFixed(fixedpoint);
                if (values < 0)
                    input.val(0);
                else
                    input.val(values);

                //trigger for angular
                input.trigger('input');

            });
        }

        function percentTextBox(selector, increasement, fixedpoint) {

            $(selector + ' .btn:first-of-type').on('click', function () {
                var input = $(selector + ' input');
                //input.val((Number($(selector + ' input').val().replace('%', '')) + increasement).toFixed(fixedpoint) + "%");

                var values = (Number($(selector + ' input').val().replace('%', '')) + increasement).toFixed(fixedpoint);
                if (values < 0)
                    input.val(0 + "%");
                else
                    input.val(values + "%");

                //trigger for angular
                input.trigger('input');
            });
            $(selector + ' .btn:last-of-type').on('click', function () {
                var input = $(selector + ' input');
                //input.val((Number($(selector + ' input').val().replace('%', '')) - increasement).toFixed(fixedpoint) + "%");

                var values = (Number($(selector + ' input').val().replace('%', '')) - increasement).toFixed(fixedpoint);
                if (values < 0)
                    input.val(0 + "%");
                else
                    input.val(values + "%");
                //trigger for angular
                input.trigger('input');
            });
        }

